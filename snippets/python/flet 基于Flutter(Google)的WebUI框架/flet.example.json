    "body": [
      "def toggle_theme(e):",
      "    \"\"\"",
      "    改变主题模式",
      "    :param e: 事件对象",
      "    \"\"\"",
      "    # import darkdetect  # * 引入 darkdetect 检测系统主题",
      "    if page.theme_mode == ft.ThemeMode.DARK:",
      "        page.theme_mode = ft.ThemeMode.LIGHT",
      "        btn_theme.icon = ft.Icons.LIGHT_MODE",
      "    elif page.theme_mode == ft.ThemeMode.LIGHT:",
      "        page.theme_mode = ft.ThemeMode.DARK",
      "        btn_theme.icon = ft.Icons.DARK_MODE",
      "    else:",
      "        # * page.theme_mode = ThemeMode.SYSTEM 跟随系统主题模式",
      "        is_dark = darkdetect.isDark()",
      "        if is_dark:",
      "            page.theme_mode = ft.ThemeMode.LIGHT",
      "            btn_theme.icon = ft.Icons.LIGHT_MODE",
      "        else:",
      "            page.theme_mode = ft.ThemeMode.DARK",
      "            btn_theme.icon = ft.Icons.DARK_MODE",
      "    page.update()",
      "btn_theme = ft.IconButton(icon=ft.Icons.DARK_MODE, tooltip='切换主题', on_click=toggle_theme)",
      "nav_rail = ft.NavigationRail(",
      "    selected_index=0,  # * 当前选中的索引",
      "    label_type=ft.NavigationRailLabelType.ALL,  # * 标签类型[ALL:显示所有标签, NONE:不显示任何标签]",
      "    min_width=100,  # * 最小宽度(无论目的地的图标或标签大小如何, 轨道的最小可能宽度)",
      "    min_extended_width=320,  # * 最小扩展宽度(extended 设置为 True 时动画完成时的最终宽度)",
      "    group_alignment=-0.9,  # * 轨道对齐方式(导航栏内项目对齐方式)[-1.0:顶/左对齐(不受缩放影响), 0.0:居中对齐, 1.0:底/右对齐(不受缩放影响)]",
      "    leading=ft.Icon(",
      "        name='ACCOUNT_CIRCLE', size=48",
      "    ),  # * 位于导航栏第一顺位控制器, 不受 group_alignment 影响, 常用 FloatingActionButton(按钮) ...",
      "    destinations=[  # * 定义导航栏内排列的按钮项的外观, 该值必须是两个或多个 NavigationRailDestination 实例的列表(最少: 2 个,否则无法显示)",
      "        ft.NavigationRailDestination(",
      "            icon=ft.Icons.FAVORITE_BORDER,",
      "            selected_icon=ft.Icons.FAVORITE,",
      "            label='First',",
      "        ),",
      "        ft.NavigationRailDestination(",
      "            icon=ft.Icon(ft.Icons.BOOKMARK_BORDER),",
      "            selected_icon=ft.Icon(ft.Icons.BOOKMARK),",
      "            label='Second',",
      "        ),",
      "    ],",
      "    trailing=btn_theme,  # * 位于导航栏最后顺位控制器, 不受 group_alignment 影响, 常用 FloatingActionButton(按钮) ...",
      "    on_change=lambda e: print('Selected destination:', e.control.selected_index),",
      ")",
      "page.add(",
