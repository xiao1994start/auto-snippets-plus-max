{
  "粗体(Bold)": {
    "prefix": [".Bold -markdown", ".strong -markdown"],
    "body": ["**${1:boldText}**"],
    "description": ["粗体(Bold)"]
  },
  "斜体(Italic)": {
    "prefix": [".italic ", ".em "],
    "body": ["*${1:italicText}*"],
    "description": ["斜体(Italic)"]
  },
  "粗体(Bold)和斜体(Italic)": {
    "prefix": [".Bold.italic ", ".strong.em "],
    "body": ["***${1:bold_italicText}***"],
    "description": ["粗体(Bold)和斜体(Italic)"]
  }
}
