{
  "WPS 控件组": {
    "prefix": ["wps_group"],
    "body": [
      "<group id=\"${1:控件组}\" label=\"${1:控件组}\">",
      "    $0",
      "    <separator id=\"separator\" />",
      "</group>",
      ""
    ],
    "description": ["WPS 控件组"]
  },
  "WPS 控件盒子": {
    "prefix": ["wps_box"],
    "body": [
      "<box id=\"box\" boxStyle=\"${1|horizontal,vertical|}\">",
      "    ",
      "</box>"
    ],
    "description": ["WPS 控件盒子"]
  },
  "WPS 分割线": {
    "prefix": ["wps_separator"],
    "body": ["<separator id=\"separator\" />", "$0"],
    "description": ["WPS 分割线", "只能在Group中使用"]
  },
  "WPS 按钮组": {
    "prefix": ["wps_btnGroup", "wps_buttonGroup"],
    "body": [
      "<buttonGroup id=\"${1:buttonGroup_}\">",
      "    $0",
      "    <separator id=\"separator\" />",
      "</buttonGroup>",
      ""
    ],
    "description": ["WPS 按钮组"]
  },
  "WPS 按钮": {
    "prefix": ["wps_btn", "wps_button"],
    "body": [
      "<button id=\"${1:唯一标识}\" label=\"${2:标签}\" onAction=\"ribbon.OnAction\"",
      "    getEnabled=\"ribbon.OnGetEnabled\" getImage=\"ribbon.GetImage\" visible=\"true\"",
      "    size=\"normal\" />",
      "$0"
    ],
    "description": [
      "WPS 按钮",
      "默认尺寸:大图标按钮",
      "大尺寸按钮: size=\"large\""
    ]
  },
  "WPS imageMso 按钮": {
    "prefix": ["wps_btn_imageMso", "wps_button_imageMso"],
    "body": [
      "<button id=\"${1:唯一标识}\" label=\"${2:标签}\" imageMso=\"${3|Bold,Italic,Underline,Fontsize|}\" onAction=\"ribbon.OnAction\"/>",
      "$0"
    ],
    "description": [
      "WPS imageMso 按钮",
      "\t仅携带原生控件图标, 需要定义onAction来实现功能"
    ]
  },
  "WPS 按钮尺寸": {
    "prefix": ["wps_btn_size", "wps_button_size"],
    "body": ["size=\"${1|large,normal|}\""],
    "description": ["WPS 按钮尺寸"]
  },
  "WPS idMso 按钮": {
    "prefix": ["wps_btn_idMso", "wps_button_idMso"],
    "body": ["<button idMso=\"${1|Bold,Italic,Underline,Fontsize|}\" />", "$0"],
    "description": ["WPS idMso 按钮", "\t携带原生控件图标和功能"]
  },
  "WPS 组合框": {
    "prefix": "wps_comboBox",
    "body": ["<comboBox idMso=\"FontSize\" id=\"xFontSize\" label=\"字号\" />"],
    "description": "WPS 组合框"
  }
}
